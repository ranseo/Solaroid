<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/album_navigation"
    android:label=""
    app:startDestination="@+id/fragment_album">


    <action
        android:id="@+id/global_action_albumAct_to_homeAct"
        app:destination="@+id/home_activity"
        app:popUpTo="@id/home_activity"
        app:popUpToInclusive="true"
        />

    <action
        android:id="@+id/global_action_albumAct_to_loginAct"
        app:destination="@+id/login_activity"
        app:popUpTo="@id/login_activity"
        app:popUpToInclusive="true"
        />

    <action
        android:id="@+id/global_action_albumAct_to_friendAct"
        app:destination="@+id/friend_activity"
        app:popUpTo="@id/friend_activity"
        app:popUpToInclusive="true"
        />


    <action
        android:id="@+id/action_album_self"
        app:destination="@+id/album_activity"
        />

    <activity
        android:id="@+id/home_activity"
        android:name="com.example.solaroid.home.activity.HomeActivity"
        tools:layout="@layout/activity_home"/>

    <activity
        android:id="@+id/login_activity"
        android:name="com.example.solaroid.login.LoginActivity"
        tools:layout="@layout/activity_login"/>
    <activity
        android:id="@+id/friend_activity"
        android:name="com.example.solaroid.friend.activity.FriendActivity"
        tools:layout="@layout/activity_friend"/>

    <activity
        android:id="@+id/album_activity"
        android:name="com.example.solaroid.album.activity.AlbumActivity"
        tools:layout="@layout/activity_album"/>



    <fragment
        android:id="@+id/fragment_album"
        android:name="com.example.solaroid.album.fragment.AlbumFragment"
        tools:layout="@layout/fragment_album"
        android:label="사진첩">

        <action
            android:id="@+id/action_album_to_gallery"
            app:destination="@+id/fragment_gallery" />

    </fragment>



</navigation>